<h5>Edit</h5>
<form action="/dinosaurs/<%= index %>/?_method=PUT" method="POST">
    <label for="dinoName">Dinosaur Name:</label>
    <input type="text" name="name" id="dinoName" value="<%= dino.name%>">
    <label for="dinoType">Dinosaur Type:</label>
    <input type="text" name="type" id="dinoType" value="<%= dino.type %> ">
    <input type="submit" value="Save">
</form>